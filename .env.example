# Legal RAG System Configuration
# Database
DATABASE_URL=postgresql://user:password@localhost:5432/legal_rag

# Jina AI API
JINA_API_KEY=your_jina_api_key_here
JINA_EMBED_BASE=https://api.jina.ai/v1/embeddings
JINA_EMBED_MODEL=jina-embeddings-v4
JINA_RERANK_BASE=https://api.jina.ai/v1/rerank
JINA_RERANK_MODEL=jina-reranker-v2-base-multilingual

# LLM Providers
LLM_PROVIDER=gemini
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# System Configuration
EMBED_BATCH_SIZE=16
RERANK_PROVIDER=jina
ENABLE_STREAM=true
LOG_LEVEL=INFO
